<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/blankFragmentHome">
    <fragment
        android:id="@+id/blankFragmentTraining"
        android:name="com.example.medicalrehabilitation.view.BlankFragmentTraining"
        android:label="fragment_blank_fragment_training"
        tools:layout="@layout/fragment_blank_fragment_training" >
        <action
            android:id="@+id/action_blankFragmentTraining_to_blankFragmentNextTraining"
            app:destination="@id/blankFragmentNextTraining" />
    </fragment>
    <fragment
        android:id="@+id/blankFragmentHome"
        android:name="com.example.medicalrehabilitation.view.BlankFragmentHome"
        android:label="fragment_blank_fragment_home"
        tools:layout="@layout/fragment_blank_fragment_home" >
        <action
            android:id="@+id/action_blankFragmentHome_to_blankFragmentTraining"
            app:destination="@id/blankFragmentTraining" />
        <action
            android:id="@+id/action_blankFragmentHome_to_blankFragmentExerciseHistoryList"
            app:destination="@id/blankFragmentExerciseHistoryList" />
    </fragment>
    <fragment
        android:id="@+id/blankFragmentNextTraining"
        android:name="com.example.medicalrehabilitation.view.BlankFragmentNextTraining"
        android:label="fragment_blank_fragment_next_training"
        tools:layout="@layout/fragment_blank_fragment_next_training" >
        <action
            android:id="@+id/action_blankFragmentNextTraining_to_blankFragmentSendMail"
            app:destination="@id/blankFragmentSendMail" />
    </fragment>
    <fragment
        android:id="@+id/blankFragmentSendMail"
        android:name="com.example.medicalrehabilitation.view.BlankFragmentSendMail"
        android:label="fragment_blank_fragment_send_mail"
        tools:layout="@layout/fragment_blank_fragment_send_mail" >
        <action
            android:id="@+id/action_blankFragmentSendMail_to_blankFragmentHome"
            app:destination="@id/blankFragmentHome" />
    </fragment>
    <fragment
        android:id="@+id/blankFragmentExerciseHistoryList"
        android:name="com.example.medicalrehabilitation.view.BlankFragmentExerciseHistoryList"
        android:label="BlankFragmentExerciseHistoryList" >
        <action
            android:id="@+id/action_blankFragmentExerciseHistoryList_to_blankFragmentHome"
            app:destination="@id/blankFragmentHome" />
    </fragment>
</navigation>